(function(w,d){var inputClient;var type;function checkCookie(){var cookieEnabled=navigator.cookieEnabled;if(!cookieEnabled){document.cookie='testcookie';cookieEnabled=document.cookie.indexOf('testcookie')!==-1}return cookieEnabled}function init(clientId,type){var client=clientId;var originUrl=encodeURIComponent(location.href);var ds=d.body.clientWidth+'x'+d.body.clientHeight;var ln=navigator.language||navigator.browserLanguage;var type=type||'';var ref=encodeURIComponent(document.referrer);var protocol=(('https:'===document.location.protocol)?'https://':'http://');var url=protocol+'isite.baidu.com/lmp/getbid?client='+client+'&client_type='+type+'&referrer='+ref+'&url='+originUrl+'&ds='+ds+'&ln='+ln+'&si='+(new Date().getTime())+'&callback=getClueInfo&cookie='+checkCookie();var script=d.createElement('script');script.setAttribute('src',url);d.getElementsByTagName('body')[0].appendChild(script)}function submitData(params){var protocol=(('https:'===document.location.protocol)?'https://':'http://');var url=protocol+'isite.baidu.com/lmp/getbid?bid='+params.bid+'&phone='+params.phone+'&callback=submitResult&cookie='+checkCookie();var script=d.createElement('script');script.setAttribute('src',url);d.getElementsByTagName('body')[0].appendChild(script)}w.clueInit=function(options){inputClient=options.client;type=options.client_type;init(inputClient,type);w.getClueInfo=options.callback};w.submitResult=function(data){console.log(data)}})(window,document);